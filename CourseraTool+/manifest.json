{
  "name": "Coursera Tool+",
  "description": "Tools for learners of Coursera!",
  "version": "1.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "512": "icons/letter-c.png"
  }
}